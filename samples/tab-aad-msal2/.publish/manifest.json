    "accentColor": "#FFFFFF",
    "staticTabs": [
        {
            "entityId": "index",
            "name": "AAD MSAL Tab Sample",
            "contentUrl": "{baseUrl1}/#tab",
            "websiteUrl": "{baseUrl1}/#web",
            "scopes": [
                "personal"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}